<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="indexReport" class="it.proximacentauri.index.report.CustomJasperReportsMultiFormatView">
		<property name="url" value="classpath:index_report.jrxml" />
	</bean>
	
	<bean id="indexCompareDateReport" class="it.proximacentauri.index.report.CustomJasperReportsMultiFormatView">
		<property name="url" value="classpath:index_compare_date_report.jrxml" />
	</bean>
	
	<bean id="indexCompareReport" class="it.proximacentauri.index.report.CustomJasperReportsMultiFormatView">
		<property name="url" value="classpath:index_compare_report.jrxml" />
	</bean>
</beans>